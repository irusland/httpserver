{
  "host": "0.0.0.0",
  "port": 8000,
  "rules": {
    "/": "pages/index.html",
    "[any_path]/page-load-errors[extras].css": {
      "path": "pages/page-load-errors[extras].css",
      "mime": "text/css"
    },
    "/request": "source/request/to_server.html",
    "/api/[path]": {
      "handler": {
        "source": "handlers/api.py",
        "get": "get"
      }
    },
    "/ping": {
      "handler": {
        "source": "handlers/api.py",
        "get": "get"
      },
      "headers": [
        ["Access-Control-Allow-Origin", "http://localhost:3000"]
      ]
    },
    "/books": {
      "handler": {
        "source": "handlers/api.py",
        "get": "get",
        "post": "post",
        "options": "preflight"
      }
    },
    "/books/[id]": {
      "handler": {
        "source": "handlers/api.py",
        "put": "put",
        "options": "preflight"
      }
    }
  },
  "error-pages": {
    "PAGE_NOT_FOUND": "pages/PAGE_NOT_FOUND.html"
  }
}